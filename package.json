    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.1",
    "prettier": "^3.0.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "packageManager": "yarn@3.5.0"
