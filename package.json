  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "packageManager": "yarn@3.6.4",
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^6.8.0"
